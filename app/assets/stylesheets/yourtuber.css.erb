* {
  font-family: 'Sawarabi Mincho', sans-serif;
}

.hrbar {
  border-top: 5px solid gray;
  margin: 50px 0;
}

.main-content {
  background-image: url(<%= asset_path('background-paper-image.jpg') %>);
  background-size: cover;
  background-repeat: no-repeat;
  background-position: center;
  min-height: 650px
}

.button-group {
  width: 70%;
}

.btn-initial {
  margin: 50px auto;
}

.select-number {
  font-weight: bold;
  float: left;
}

<%# index %>
.modal-question-title {
  background: linear-gradient(transparent 70%, yellow 70%);
}

.index-container{
  display: none;
}

.index-container-table{
  overflow-x: scroll;
  white-space: nowrap;
}
